{
  "swagger": "2.0",
  "info": {
    "version": "2.0.1",
    "title": "Question Generator API",
    "description": "API Documentation"
  },
  "host": "localhost:8080",
  "basePath": "/",
  "tags": [
    {
      "name": "Question",
      "description": "Endpoints"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/generate-questions": {
      "get": {
        "tags": [],
        "description": "",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/employee": {
      "post": {
        "tags": [
          "Employee"
        ],
        "description": "Endpoint to add new employees.",
        "parameters": [
          {
            "name": "newUser",
            "in": "body",
            "description": "Informações do usuário.",
            "required": true,
            "schema": {
              "$ref": "#/definitions/AddUser"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/employee/{employeeId}": {
      "get": {
        "tags": [
          "Employee"
        ],
        "description": "Endpoint to get single employees.",
        "parameters": [
          {
            "name": "employeeId",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "id",
            "description": "Employee ID",
            "in": "query",
            "type": "string"
          },
          {
            "name": "username",
            "description": "Username to find",
            "type": "string",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/employees": {
      "get": {
        "tags": [
          "Employee"
        ],
        "description": "Endpoint to get all employees.",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Usuário encontrado.",
            "schema": {
              "$ref": "#/definitions/User"
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  },
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "question": {
          "type": "string",
          "example": "What is the speed of light"
        },
        "subject": {
          "type": "string",
          "example": "Physics"
        },
        "topic": {
          "type": "string",
          "example": "Waves"
        },
        "difficulty": {
          "type": "string",
          "example": "Easy"
        },
        "marks": {
          "type": "number",
          "example": 5
        }
      }
    }
  }
}